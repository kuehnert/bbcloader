{
  "name": "bbcloader",
  "version": "1.2.1",
  "scripts": {
    "build": "npm run tsc",
    "clean": "rimraf dist/*",
    "clear": "rimraf dist/* node_modules/{*,.bin}",
    "deploy:production": "shipit production deploy",
    "deploy:poormans": "env-path -p .env.master npm run build && rsync -az --exclude \".env\"  --force --delete --progress -e \"ssh -p22\" dist/ mk@192.168.168.4:~/sites/bbcloader-api/poormans",
    "start:production": "cross-env NODE_ENV=production node dist/app.js",
    "start": "NODE_ENV=development nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/app.ts",
    "tsc:w": "tsc --project tsconfig.json -w",
    "tsc": "tsc --project tsconfig.json"
  },
  "author": "Matthias KÃ¼hnert",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.3.6",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "date-fns": "2.14.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "redux-thunk": "^2.3.0",
    "request": "2.88.2",
    "shipit-deploy": "5.3.0",
    "shipit-shared": "4.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/fast-html-parser": "1.0.0",
    "@types/lodash": "4.14.155",
    "@types/node": "14.0.13",
    "@types/request-promise": "4.1.46",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "env-path": "^1.0.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.21.2",
    "fast-html-parser": "1.0.1",
    "nodemon": "2.0.4",
    "rimraf": "3.0.2",
    "shipit-cli": "5.3.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "yargs": "15.3.1"
  }
}

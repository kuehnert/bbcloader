class Video {
  constructor(url) {
    [, this.id] = url.match(/episode\/(\w+)\//);
    this.url = url;
    this.programme = '';
    this.series = -1;
    this.episodeNumber = -1;
    this.episodeTitle = '';
    this.filename = '';
    this.attempts = 0;
    // this.tagged = false;
    // this.downloaded = false;
    // this.subtitle_converted = false;
    // this.sorted = false;
  }
}

export default Video;

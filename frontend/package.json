{
  "name": "bbcloader-frontend",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "npm run clean && react-scripts build",
    "clean": "rimraf build/{*,.*}",
    "clear": "rimraf node_modules/{*,.*} build/{*,.*} package-lock.json",
    "deploy:production": "env-path -p .env.production npm run build && rsync -az --exclude \".env\"  --force --delete --progress -e \"ssh -p22\" build/ mk@192.168.168.4:~/sites/bbcloader-webapp/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@mdi/js": "5.9.55",
    "@mdi/react": "1.5.0",
    "@reduxjs/toolkit": "1.5.1",
    "axios": "0.21.1",
    "classnames": "2.3.1",
    "date-fns": "^2.21.3",
    "dotenv": "10.0.0",
    "formik": "2.2.8",
    "jsonwebtoken": "^8.5.1",
    "lodash": "4.17.21",
    "normalizr": "3.6.1",
    "primeflex": "2.0.0",
    "primeicons": "4.1.0",
    "primereact": "6.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "4.4.1",
    "redux": "4.1.0",
    "validator": "13.6.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "4.14.170",
    "@types/node": "15.6.1",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "env-path": "1.0.7",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "node-sass": "6.0.0",
    "prettier": "2.3.0",
